{
  "theme": "Default",
  "mcpServers": {
    "code-context": {
      "command": "npx",
      "args": ["@zilliz/code-context-mcp@latest"],
      "env": {
        "OPENAI_API_KEY": "${GEMINI_API_KEY}",
        "MILVUS_TOKEN": "${MILVUS_TOKEN}"
      }
    },
    "cipher": {
      "type": "stdio",
      "command": "npx",
      "args": ["cipher", "--mode", "mcp", "--agent", "memAgent/cipher.yml"],
      "env": {
        "OPENAI_API_KEY": "${GEMINI_API_KEY}",
        "OLLAMA_BASE_URL": "${OLLAMA_BASE_URL}"
      }
    }
  },
  "contextFileName": [
    "./../.kiro/specs/fullstack-portfolio-app/design.md",
    "./../.kiro/specs/fullstack-portfolio-app/requirements.md",
    "./../.kiro/specs/fullstack-portfolio-app/tasks.md",
    "./../.kiro/steering/product.md",
    "./../.kiro/steering/structure.md",
    "./../.kiro/steering/tech.md"
  ]
}
